{
  "npmScope": "nx-skeleton",
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e",
          "build-storybook"
        ]
      }
    }
  },
  "projects": {
    "dashboard": {
      "tags": []
    },
    "dashboard-e2e": {
      "tags": [],
      "implicitDependencies": ["dashboard"]
    },
    "dashboard-protractor-e2e": {
      "tags": [],
      "implicitDependencies": ["dashboard"]
    },
    "shared-ui-core": {
      "tags": []
    },
    "shared-utils": {
      "tags": []
    },
    "shared-assets": {
      "tags": []
    }
  }
}
